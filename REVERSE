public class ReverseWord {
    public static void main(String[] args) {
        String word = "example";
        String reversedword="";
        for(int i=word.length()-1;i>=0;i--)
        {
          reversedword +=word.charAt(i);
        }
        System.out.println("original word:"+word);
        System.out.println("reversed word:"+reversedword);
    }}
